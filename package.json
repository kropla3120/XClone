    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.30.9",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "lucide-react": "^0.378.0",
